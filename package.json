{
  "name": "remark-youtube",
  "version": "1.3.1",
  "description": "Remark plugin for youtube",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": "pkolt/remark-youtube",
  "scripts": {
    "test": "node --loader ts-node/esm --test --no-warnings ./src/*.test.ts",
    "build": "rimraf ./dist && tsc",
    "prepare": "npm run build",
    "format": "prettier --check src",
    "format-fix": "prettier --write src",
    "release": "release-it"
  },
  "keywords": [
    "remark",
    "rehype",
    "youtube",
    "iframe",
    "embedded"
  ],
  "bugs": {
    "url": "https://github.com/pkolt/remark-youtube/issues",
    "email": "pkoltyshev@gmail.com"
  },
  "author": "Pavel Koltyshev <pkoltyshev@gmail.com>",
  "files": [
    "README.md",
    "LICENSE.md",
    "dist"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 18"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.3",
    "@types/node": "^20.11.30",
    "prettier": "^3.2.5",
    "rehype-stringify": "^10.0.0",
    "release-it": "^17.1.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-rename": "^0.2.0",
    "typescript": "^5.4.3",
    "unified": "^11.0.4"
  },
  "dependencies": {
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "remark-rehype": "^11.1.0"
  }
}
